Cell[BoxData[
 RowBox[{
  RowBox[{"GenerateListOfRadii", "[", "method_String", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"r", ",", "replaceValue"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"r", "=", 
      RowBox[{"Association", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"#", "\[Rule]", 
          RowBox[{"ElementData", "[", 
           RowBox[{"#", ",", "method"}], "]"}]}], "&"}], "/@", 
        RowBox[{"Keys", "@", "$PeriodicTable"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"replaceValue", "=", 
      RowBox[{"Median", "@", 
       RowBox[{"Cases", "[", 
        RowBox[{"r", ",", "_Quantity"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"r", "=", 
      RowBox[{"r", "/.", 
       RowBox[{"_Missing", "\[Rule]", "replaceValue"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"r", "=", 
      RowBox[{"QuantityMagnitude", "/@", "r"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"r", "/=", "100."}]}]}], 
   RowBox[{"(*", " ", 
    RowBox[{"Convert", " ", "to", " ", "\[ARing]ngstr\[ODoubleDot]ms"}], " ", 
    "*)"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.80011262243464*^9, 3.8001126725188713`*^9}, {
   3.800112734954368*^9, 3.80011276318262*^9}, {3.802712296399942*^9, 
