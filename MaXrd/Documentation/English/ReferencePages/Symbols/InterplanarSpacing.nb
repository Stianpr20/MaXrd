  ExpressionUUID->"3ad8d5dc-71c8-49c6-b84f-6efd9b0d3ad7"],
 ", with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["h", "\[RightVector]"], "=", 
    RowBox[{"[", 
     RowBox[{"h", ",", "k", ",", "l"}], "]"}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"0120d966-bff7-4226-a1de-9177e38b7f0f"],
 " and ",
