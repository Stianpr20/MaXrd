Cell[TextData[{
 "The wavelength ",
 Cell[BoxData[
  StyleBox["\[Lambda]", "TR"]], "InlineFormula",ExpressionUUID->
  "e43b94df-8c36-4afb-a1de-dcbcd7e805d4"],
