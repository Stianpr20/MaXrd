Cell[BoxData[
 FormBox[
  StyleBox[
   RowBox[{
    SubscriptBox[
     StyleBox["\[CapitalLambda]",
      FontWeight->"Plain"], "o"], "=", 
    FractionBox[
     RowBox[{"\[Pi]", " ", "V", 
      SqrtBox[
       RowBox[{
        SubscriptBox["\[Gamma]", "o"], 
        RowBox[{"\[LeftBracketingBar]", 
         SubscriptBox["\[Gamma]", "h"], "\[RightBracketingBar]"}]}]]}], 
     RowBox[{"C", " ", "\[Lambda]", " ", 
      SubscriptBox["r", "e"], 
      SqrtBox[
       RowBox[{
        SubscriptBox["F", "H"], " ", 
        SubscriptBox["F", 
         OverscriptBox["H", "_"]]}]]}]]}],
   FontWeight->"Plain"], TraditionalForm]], "Input",
