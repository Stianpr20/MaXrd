      RowBox[{"(", 
       RowBox[{
        RowBox[{"Cos", "[", "\[Alpha]", "]"}], "-", 
        RowBox[{
         RowBox[{"Cos", "[", "\[Beta]", "]"}], " ", 
         RowBox[{"Cos", "[", "\[Gamma]", "]"}]}]}], ")"}], " ", 
      RowBox[{"Csc", "[", "\[Gamma]", "]"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"0", ",", "0", ",", 
     RowBox[{"c", " ", 
      SqrtBox[
       RowBox[{"1", "-", 
        SuperscriptBox[
         RowBox[{"Cos", "[", "\[Beta]", "]"}], "2"], "-", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Cos", "[", "\[Alpha]", "]"}], "-", 
            RowBox[{
             RowBox[{"Cos", "[", "\[Beta]", "]"}], " ", 
             RowBox[{"Cos", "[", "\[Gamma]", "]"}]}]}], ")"}], "2"], " ", 
         SuperscriptBox[
          RowBox[{"Csc", "[", "\[Gamma]", "]"}], "2"]}]}]]}]}], "}"}]}], 
