  RowBox[{
   RowBox[{"{", 
    RowBox[{"a", ",", 
     RowBox[{"b", " ", 
      RowBox[{"Cos", "[", "\[Gamma]", "]"}]}], ",", 
     RowBox[{"c", " ", 
      RowBox[{"Cos", "[", "\[Beta]", "]"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"0", ",", 
