Cell[BoxData[
 FormBox[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {"a", "0", "0"},
     {
      RowBox[{"b", " ", 
       RowBox[{"cos", "(", "\[Gamma]", ")"}]}], 
      RowBox[{"b", " ", 
       RowBox[{"sin", "(", "\[Gamma]", ")"}]}], "0"},
     {
      RowBox[{"c", " ", 
       RowBox[{"cos", "(", "\[Beta]", ")"}]}], 
      RowBox[{"c", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"cos", "(", "\[Alpha]", ")"}], "-", 
         RowBox[{
          RowBox[{"cos", "(", "\[Beta]", ")"}], " ", 
          RowBox[{"cos", "(", "\[Gamma]", ")"}]}]}], ")"}], " ", 
       RowBox[{"csc", "(", "\[Gamma]", ")"}]}], 
      RowBox[{"c", " ", 
       SqrtBox[
        RowBox[{
         RowBox[{"-", 
          RowBox[{
           SuperscriptBox["cos", "2"], "(", "\[Beta]", ")"}]}], "-", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{
             RowBox[{"cos", "(", "\[Alpha]", ")"}], "-", 
             RowBox[{
              RowBox[{"cos", "(", "\[Beta]", ")"}], " ", 
              RowBox[{"cos", "(", "\[Gamma]", ")"}]}]}], ")"}], "2"], " ", 
          RowBox[{
           SuperscriptBox["csc", "2"], "(", "\[Gamma]", ")"}]}], "+", "1"}]]}]}
    },
