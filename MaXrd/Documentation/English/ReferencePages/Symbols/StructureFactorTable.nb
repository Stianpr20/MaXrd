Cell[TextData[Cell[BoxData[
 ButtonBox["Nothing",
  BaseStyle->
   "Link"]], \
"InlineFormula",ExpressionUUID->"40f8ad78-4046-48df-b50d-62c81334babc"]], \
"Input",
 CellChangeTimes->{3.861683265900063*^9},
 CellID->754624388,ExpressionUUID->"3c4f73af-c075-4838-b16e-de48dbffface"],

Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"60421946-6d3c-402d-9568-82c63fdd533c"],
 "\!\(\*RowBox[{\"StructureFactorTable\", \"[\", StyleBox[\"crystal\", \
\"TI\"], \"]\"}]\)\!\(\*RowBox[{\" produces a table of structure factors, \
phases, Bragg angles, extinction lengths and Darwin widths for a given \", \
Cell[BoxData[StyleBox[\"crystal\", \"TI\"]], \"InlineFormula\"], \".\"}]\) \!\
\(\*RowBox[{\"StructureFactorTable\", \"[\", StyleBox[\"crystal\", \"TI\"], \
\",\", StyleBox[\"\[Lambda]\", \"TR\"], \"]\"}]\) \!\(\*RowBox[{\"produces \
the same table for a given \", Cell[BoxData[StyleBox[\"crystal\", \"TI\"]], \
\"InlineFormula\"], \" but using wavelength \", Cell[BoxData[StyleBox[\"\
\[Lambda]\", \"TR\"]], \"InlineFormula\"], \".\"}]\) \
\!\(\*RowBox[{\"StructureFactorTable\", \"[\", StyleBox[\"crystal\", \"TI\"], \
\",\", StyleBox[\"\[Lambda]\", \"TR\"], \",\", StyleBox[\"condition\", \
\"TI\"], \"]\"}]\) \!\(\*RowBox[{\"produces the same table, but reflections \
that do not follow the pattern of \", Cell[BoxData[StyleBox[\"condition\", \
\"TI\"]], \"InlineFormula\"], \" will be disregarded.\"}]\)"
}], "Usage",
