  FormBox[
   RowBox[{"\[Lambda]", "=", 
    RowBox[{"0.15", " ", "\[CapitalARing]"}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"cdd4f441-bdcb-4f6c-9fcd-41be57ab86af"],
