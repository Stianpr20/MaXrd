    RowBox[{
     RowBox[{"Which", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ListQ", "@", "sourceUnitsInput"}], ",", "\[IndentingNewLine]", 
       RowBox[{"If", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Depth", "@", "sourceUnitsInput"}], "\[NotEqual]", "2"}], 
           ")"}], "||", "\[IndentingNewLine]", 
          RowBox[{"(", 
           RowBox[{"!", 
            RowBox[{"AllTrue", "[", 
             RowBox[{"sourceUnitsInput", ",", "StringQ"}], "]"}]}], ")"}], "||",
           "\[IndentingNewLine]", 
          RowBox[{"(", 
           RowBox[{"sourceUnitsInput", "===", 
            RowBox[{"{", "}"}]}], ")"}]}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Message", "[", 
           RowBox[{"EmbedStructure", "::", "InvalidSourceInput"}], "]"}], ";", 
          RowBox[{"Abort", "[", "]"}]}]}], "]"}], ",", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Head", "@", "sourceUnitsInput"}], "===", "Rule"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"!", 
           RowBox[{"MatchQ", "[", 
            RowBox[{
             RowBox[{"Length", "/@", "sourceUnitsInput"}], ",", 
             RowBox[{"x_", "\[Rule]", 
              RowBox[{"x_", "/;", 
               RowBox[{"x", "\[Equal]", "x"}]}]}]}], "]"}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Message", "[", 
            RowBox[{"EmbedStructure", "::", "InvalidSourceInput"}], "]"}], 
           ";", 
           RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"!", 
           RowBox[{"AllTrue", "[", 
            RowBox[{
             RowBox[{
             "sourceUnitsInput", "\[LeftDoubleBracket]", "1", 
              "\[RightDoubleBracket]"}], ",", 
             RowBox[{
              RowBox[{"0.0", "\[LessEqual]", "#", "\[LessEqual]", "1.0"}], 
              "&"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Message", "[", 
            RowBox[{"EmbedStructure", "::", "InvalidProbabilities"}], "]"}], 
           ";", 
           RowBox[{"Abort", "[", "]"}]}]}], "]"}]}], ",", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", "True", ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"EmbedStructure", "::", "InvalidSourceInput"}], "]"}], ";", 
