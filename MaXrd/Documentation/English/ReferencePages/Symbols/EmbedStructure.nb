Cell[TextData[{
 "In this example, a carbon atom is placed in the centre of a silicon unit \
cell, then shifted by a random amount in the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]], "InlineMath",ExpressionUUID->
  "ec214dae-02a2-4e39-b153-aabc7bd9b471"],
 " and ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath",ExpressionUUID->
  "17f9298c-2d0d-4de3-9a80-8ea37c74aace"],
 " directions by an amount up to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[PlusMinus]", "0.25`"}], " ", "\[CapitalARing]"}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "a673dd6b-5aa7-4add-9185-414555494856"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[PlusMinus]", "0.75`"}], " ", "\[CapitalARing]"}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "d1fa2874-b6ae-4a6b-883e-7c2fbac83f09"],
 ", respectively."
}], "ExampleText",
