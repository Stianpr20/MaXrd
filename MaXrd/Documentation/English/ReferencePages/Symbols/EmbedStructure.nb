Cell[BoxData[GridBox[{
   {Cell["      ", "ModInfo",ExpressionUUID->
     "0bda74d4-d621-4e69-8430-76f322e0bd76"], 
    RowBox[{"\"\<Distortions\>\"", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"{", 
        RowBox[{"2", ",", "5"}], "}"}], ",", "3"}], "}"}]}], Cell[TextData[{
     "distorts each embedded structure by a random amount between ",
     Cell[BoxData[
      FormBox[
       RowBox[{"2", " ", "\[Dash]", 
        RowBox[{"5", "\[CapitalARing]"}]}], TraditionalForm]], "InlineMath",
      ExpressionUUID->"9fdd6602-783e-4298-9cae-66d99b676152"],
     " in the ",
     Cell[BoxData[
      FormBox["y", TraditionalForm]], "InlineMath",ExpressionUUID->
      "0f3baa63-0e1c-4b42-bfef-fd75a95cf7bf"],
     "-direction and a constant amount by ",
     Cell[BoxData[
      FormBox[
       RowBox[{"3", "\[CapitalARing]"}], TraditionalForm]], "InlineMath",
      ExpressionUUID->"a85d48d1-e220-4081-b9bb-3b6d3024232f"],
     " in the ",
     Cell[BoxData[
      FormBox["z", TraditionalForm]], "InlineMath",ExpressionUUID->
      "6e4435f5-67be-434a-93f7-9bd6c9ea5702"],
     "-direction."
    }], "TableText",ExpressionUUID->"b54e6922-45a6-4b6c-8c0c-798831f76d89"]},
   {Cell["      ", "ModInfo",ExpressionUUID->
     "303b80e9-c13b-4501-b376-27101ee30e2b"], 
    RowBox[{"\"\<Rotations\>\"", "\[Rule]", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x_", ",", "y_", ",", "z_"}], "}"}], "/;", 
        RowBox[{"y", ">", "2"}]}], "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"90", "\[Degree]"}], ",", "0", ",", 
         RowBox[{"25", "\[Degree]"}]}], "}"}]}], "\[IndentingNewLine]", 
      "}"}]}], Cell[TextData[{
     "rotates all embedded structures placed at a position where ",
     Cell[BoxData[
      FormBox[
       RowBox[{"y", ">", "2"}], TraditionalForm]], "InlineMath",
      ExpressionUUID->"e2f6a0a9-6536-4639-a574-8046772ef0ff"],
     " by ",
     Cell[BoxData[
      FormBox[
       RowBox[{"90", "\[Degree]"}], TraditionalForm]], "InlineMath",
      ExpressionUUID->"fe3af095-4606-4489-9ded-3aa8974d204a"],
     " about the ",
     Cell[BoxData[
      FormBox["x", TraditionalForm]], "InlineMath",ExpressionUUID->
      "defa04e9-5e87-401d-b963-27eb2a9da93a"],
     "-axis and ",
     Cell[BoxData[
      FormBox[
       RowBox[{"25", "\[Degree]"}], TraditionalForm]], "InlineMath",
      ExpressionUUID->"6f56dad9-252a-4e89-9617-b029e21a755e"],
     " about the ",
     Cell[BoxData[
      FormBox["z", TraditionalForm]], "InlineMath",ExpressionUUID->
      "cefdd9e5-572d-40d3-bac5-e9376e264405"],
     "-axis."
    }], "TableText",ExpressionUUID->"a6d9f2be-a9da-4433-923e-77bb2dc10444"]},
   {Cell["      ", "ModInfo",ExpressionUUID->
     "f2454663-8d1b-4dea-b9e9-08a0d0e74ab5"], 
    RowBox[{"\"\<Rotations\>\"", "\[Rule]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", "0", "}"}], ",", 
          RowBox[{"{", "60", "}"}], ",", 
          RowBox[{"{", "120", "}"}]}], "}"}], ",", "0", ",", "0"}], "}"}], 
      "\[Degree]"}]}], Cell[TextData[{
     "rotates each embedded entity by either 0\[Degree], 60\[Degree] or 120\
\[Degree] about the ",
     Cell[BoxData[
      FormBox["x", TraditionalForm]], "InlineMath",ExpressionUUID->
      "a466bc39-3ecf-487e-80a7-72cce77a9197"],
     "-axis (random choice each time)."
    }], "TableText",ExpressionUUID->"bf3accb2-2c7a-441e-b35f-57fd3a9b404b"]},
   {Cell["      ", "ModInfo",ExpressionUUID->
     "abecf0f7-4ab9-4e4c-839e-938112ef0522"], 
    RowBox[{"\"\<Distortions\>\"", "\[Rule]", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\"\<GuestEntityA\>\"", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"0", ",", "0.25", ",", "0"}], "}"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"\"\<GuestEntityB\>\"", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"{", 
           RowBox[{"0.4", ",", "0.6"}], "}"}], ",", "0"}], "}"}]}]}], 
      "\[IndentingNewLine]", "}"}]}], Cell[TextData[{
     "shifts all entites with the first label ",
     Cell[BoxData[
      FormBox[
       RowBox[{"0.25", "\[CapitalARing]"}], TraditionalForm]], "InlineMath",
      ExpressionUUID->"2a7a1a2f-1be5-4f37-a744-37ee427800d8"],
     " in the ",
     Cell[BoxData[
      FormBox["y", TraditionalForm]], "InlineMath",ExpressionUUID->
      "bf94f231-8e00-4b17-b394-d272f1f688d1"],
     "-direction, and all entities of the second label by a random amount in \
that range."
    }], "TableText",ExpressionUUID->"df16ebb0-af20-412b-ae2e-c9652c72db1b"]}
  }]], "2ColumnTableMod",
