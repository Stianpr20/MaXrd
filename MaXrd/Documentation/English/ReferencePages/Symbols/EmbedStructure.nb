 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"0.75", "a"}], "=", 
    RowBox[{
     RowBox[{"0.75", "\[CenterDot]", 
      RowBox[{"(", 
       RowBox[{"5.0", "\[CapitalARing]"}], ")"}]}], "=", 
     RowBox[{"3.75", "\[CapitalARing]"}]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"c66d0fc6-1b74-4e82-a4ca-9bfafbf96563"],
