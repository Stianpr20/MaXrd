Cell[BoxData[
 RowBox[{
  RowBox[{"EmbedStructure", "[", "\[IndentingNewLine]", 
   RowBox[{
   "sourceUnitsInput_", ",", "TargetPositions_List", ",", 
    "\[IndentingNewLine]", "targetCrystal_String", ",", 
    "outputCrystal_String", ",", "\[IndentingNewLine]", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "\[IndentingNewLine]", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
     "hostStructureSize", ",", "\[IndentingNewLine]", "sourceUnits", ",", 
      "sourceCopies", ",", "\[IndentingNewLine]", 
      RowBox[{"matchHostSizeQ", "=", 
       RowBox[{"TrueQ", "@", 
        RowBox[{"OptionValue", "[", "\"\<MatchHostSize\>\"", "]"}]}]}], ",", 
      "\[IndentingNewLine]", "targetPositions", ",", "copyTranslations", ",", 
      "\[IndentingNewLine]", "latticeParameters", ",", "latticeParametersABC",
