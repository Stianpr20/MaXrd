   RowBox[{"ConstructDomains", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"7", ",", "7", ",", "7"}], "}"}], ",", "3", ",", "250"}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
