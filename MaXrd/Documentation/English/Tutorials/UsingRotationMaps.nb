   RowBox[{"InputCheck", "[", 
    RowBox[{"\"\<RotationTransformation\>\"", ",", "domain2", ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", "\"\<Unit\>\"", ",", 
       RowBox[{"<|", 
        RowBox[{"1", "\[Rule]", 
         RowBox[{"{", 
