Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"m", "=", 
   RowBox[{"MinMax", "@", 
    RowBox[{"DeleteDuplicates", "@", 
     RowBox[{"Last", "@", "domain"}]}]}]}], ";"}], "\[IndentingNewLine]", 
