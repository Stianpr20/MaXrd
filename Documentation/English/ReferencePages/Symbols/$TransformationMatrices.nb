          RowBox[{"cos", "(", "\[Gamma]", ")"}]}]}], ")"}]}], 
      RowBox[{"c", " ", 
       SqrtBox[
        RowBox[{
         RowBox[{"-", 
          RowBox[{
           RowBox[{
            SuperscriptBox["csc", "2"], "(", "\[Gamma]", ")"}], " ", 
